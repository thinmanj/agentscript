{
  "comments": {
    "lineComment": "//"
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"]
  ],
  "autoClosingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""]
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""]
  ],
  "indentationRules": {
    "increaseIndentPattern": "^(.*\\{[^}]*|.*\\([^)]*|.*\\[[^\\]]*)$",
    "decreaseIndentPattern": "^(.*\\}.*|.*\\).*|.*\\].*)$"
  },
  "wordPattern": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\b"
}